{
  "extends": ["next/core-web-vitals", "airbnb-base"],
  "rules": {
    "no-unused-vars": [2, {
      "args": "none"
    }],
    "import/prefer-default-export": "off",
    "import/extensions": "off",
    "max-len": "off",
    "arrow-body-style": "off",
    "import/no-extraneous-dependencies": "off",
    "quotes": ["error", "single"],
    "react/jsx-indent": ["error", 2],
    "indent": ["error", 2, {
      "SwitchCase": 1,
      "ignoreComments": false
    }]
  }
}
